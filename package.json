    "devDependencies": {
        "@maticnetwork/maticjs": "^2.0.40",
        "@truffle/hdwallet-provider": "^1.4.2",
        "dotenv": "^9.0.0",
        "truffle-plugin-verify": "^0.6.1"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^4.8.0",
        "hardhat": "^2.12.3"
